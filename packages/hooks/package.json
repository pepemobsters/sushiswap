{
  "name": "@sushiswap/hooks",
  "version": "3.0.0",
  "private": true,
  "description": "Sushi Hooks",
  "keywords": [
    "sushi",
    "hooks",
    "react"
  ],
  "license": "MIT",
  "author": "Matthew Lilley <hello@matthewLilley.com>",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*.js",
      "./dist/*/index.js"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc -w"
  },
  "dependencies": {
    "copy-to-clipboard": "3.3.3",
    "react-responsive": "9.0.0-beta.10",
    "viem": "2.21.55"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.4",
    "@tsconfig/strictest": "2.0.2",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sushi": "4.1.22",
    "tailwindcss": "3.3.2",
    "typescript": "5.7.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "sushi": "*",
    "tailwindcss": "*"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    },
    "sushi": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    }
  }
}
